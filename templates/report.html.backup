<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"> 
<html xmlns="http://www.w3.org/1999/xhtml" lang="en-au" xml:lang="en-au" > 
<head> 
<title>Picker Report</title> 
<link rel="stylesheet" type="text/css" href="/media/admin/css/base.css" /> 
<link rel="stylesheet" type="text/css" href="/media/admin/css/dashboard.css" /> 
<!--[if lte IE 7]><link rel="stylesheet" type="text/css" href="http://glebaBE/media/admin/css/ie.css" /><![endif]-->  
<meta name="robots" content="NONE,NOARCHIVE" /> 
</head> 
 
<body class="dashboard"> 
 
<!-- Container --> 
<div id="container"> 
    <!-- Header --> 
    <div id="header"> 
        <div id="branding"> 
		{% if report_type_picker %}
		<h1 id="site-name">Picker Report</h1> 
	</div> 
	</div> 
	<!-- Content --> 
	    <div id="content" class="colMS"> 
		<h1>Picker Report :: {{ picker.firstName }} {{ picker.lastName }}</h1> 
		<div id="content-main"> 
	 
		    	<img  src="{{ graph_filename }}"/>
			<div 	
		<table>
			{% for key,value in picker_data.items %}
			<tr>
 				<td>{{key}}</td><td>{{value}}</td>
                        </tr>
			{% endfor %}
		</table>
		{% endif %}

		{% if picker_type_room %}
		<h1 id="site-name">Room Report</h1> 
	</div> 
	</div> 
	<!-- Content --> 
	    <div id="content" class="colMS"> 
		<h1>Room Report :: {{ room.number }}</h1> 
		<div id="content-main"> 
 
		    <img  src="{{ graph_filename }}"/>
		{% endif %}

		{% if report_type_flush %}
		<h1 id="site-name">Room Report</h1> 
	</div> 
	</div> 
	<!-- Content --> 
	    <div id="content" class="colMS"> 
		<h1>Flush Report :: {{ room.number }}</h1> 
		<div id="content-main"> 
 
		    <img  src="{{ graph_filename }}"/>
		{% endif %}

{% if default_page %}
show forms here to select report type
{% endif %}
</div>
{{ debug }}
</body>
