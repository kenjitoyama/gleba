<?xml version="1.0" encoding="ISO-8859-1"?>
<varietyList>
    {% for variety in variety_list%}<variety>
        <id>{{variety.id}}</id>
        <name>{{variety.name}}</name>
        <minimum_weight>{{variety.minimum_weight}}</minimum_weight>
        <tolerance>{{variety.tolerance}}</tolerance>
    </variety>
    {% endfor %}
</varietyList>

